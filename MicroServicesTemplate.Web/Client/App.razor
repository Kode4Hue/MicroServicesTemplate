@using MicroServicesTemplate.Web.Client.Features.Common.Layouts
<Router AppAssembly="@typeof(Program).Assembly" PreferExactMatches="@true">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(LayoutOne)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(LayoutOne)">
            <p>Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Router>
